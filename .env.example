# ============================================
# Supabase 配置（必需）
# ============================================
# Supabase 项目 ID
NEXT_PUBLIC_SUPABASE_PROJECT_ID="你的Supabase项目ID"

# Supabase 公开密钥（Anon Key）
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY="你的Supabase公开密钥"

# Supabase 项目 URL
NEXT_PUBLIC_SUPABASE_URL="https://你的项目ID.supabase.co"

# Supabase 数据库密码（如果需要）
NEXT_PUBLIC_SUPABASE_DATABASE_PADDWD=你的数据库密码

# Supabase 服务角色密钥（服务端使用，敏感信息）
SUPABASE_SERVICE_ROLE_KEY=你的Supabase服务角色密钥

# ============================================
# 微信登录配置（可选）
# ============================================
# 微信网页应用 ID（公开）
NEXT_PUBLIC_WECHAT_APPID="你的微信应用ID"

# 微信应用密钥（敏感信息，仅服务端使用）
WECHAT_APPID=你的微信应用ID
WECHAT_APP_SECRET=你的微信应用密钥

# 微信回调 URL
NEXT_PUBLIC_SUPABASE_WECHAT_CALLBACK=https://你的域名.com/auth/wechat/callback

# ============================================
# API 基础配置（可选）
# ============================================
# API 基础 URL
NEXT_PUBLIC_API_BASE_URL=https://你的域名.com

# ============================================
# 聊天会话管理提示词（可选）
# ============================================
# 会话标题生成提示词
SESSION_TITLE_PROMPT=使用四到五个字直接返回这句话的简要主题，不要解释、不要标点、不要语气词、不要多余文本，不要加粗，如果没有主题，请直接返回"闲聊"

# 会话记忆总结提示词
SESSION_MEMORY_PROMPT=简要总结一下对话内容，用作后续的上下文提示 prompt，控制在 200 字以内

# ============================================
# OpenRouter 配置（可选）
# ============================================
# OpenRouter API URL
OPENROUTER_URL=https://openrouter.ai/api

# OpenRouter API 密钥
OPENROUTER_API_KEY=你的OpenRouter_API密钥

# OpenRouter 默认模型
NEXT_PUBLIC_OPENROUTER_DEFAULT_MODEL=x-ai/grok-4.1-fast

# ============================================
# OpenAI 配置（可选）
# ============================================
# OpenAI API 密钥
OPENAI_API_KEY=你的OpenAI_API密钥

# OpenAI 组织 ID（可选）
OPENAI_ORG_ID=你的OpenAI组织ID

# OpenAI API 基础 URL（可选，用于代理）
BASE_URL=https://api.openai.com

# ============================================
# DeepSeek 配置（可选）
# ============================================
# DeepSeek API 密钥
DEEPSEEK_API_KEY=你的DeepSeek_API密钥

# ============================================
# SiliconFlow 配置（可选）
# ============================================
# SiliconFlow API 密钥
SILICONFLOW_API_KEY=你的SiliconFlow_API密钥

# SiliconFlow API URL
SILICONFLOW_URL=https://api.siliconflow.cn

# ============================================
# Anthropic Claude 配置（可选）
# ============================================
# Anthropic API 密钥
ANTHROPIC_API_KEY=你的Anthropic_API密钥

# Anthropic API 版本
ANTHROPIC_API_VERSION=

# Anthropic API URL
ANTHROPIC_URL=

# ============================================
# Google Gemini 配置（可选）
# ============================================
# Google Gemini API 密钥
GOOGLE_API_KEY=你的Google_API密钥

# Google Gemini API URL
GOOGLE_URL=https://generativelanguage.googleapis.com/

# ============================================
# AI302 配置（可选）
# ============================================
# AI302 API 密钥
AI302_API_KEY=你的AI302_API密钥

# AI302 API URL
AI302_URL=

# ============================================
# 功能开关配置（可选）
# ============================================
# 访问密码，多个密码用逗号分隔
CODE=

# 代理 URL（如果需要通过代理访问）
PROXY_URL=

# 启用 MCP 功能（设置为 "true" 启用）
ENABLE_MCP=false

# 禁用 GPT-4（设置为 1 禁用）
DISABLE_GPT4=

# 隐藏用户 API 密钥输入（设置为 1 隐藏）
HIDE_USER_API_KEY=

# 启用余额查询（设置为 1 启用）
ENABLE_BALANCE_QUERY=

# 禁用快速链接解析（设置为 1 禁用）
DISABLE_FAST_LINK=

# 自定义模型配置（使用 + 添加，- 隐藏，name=displayName 自定义名称，逗号分隔）
CUSTOM_MODELS=

# 默认模型
DEFAULT_MODEL=Qwen/Qwen2.5-72B-Instruct

# WebDAV 白名单端点
WHITE_WEBDAV_ENDPOINTS=

